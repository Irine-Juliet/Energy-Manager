[run]
source = energy_tracker
omit = 
    */migrations/*
    */tests/*
    */test_*.py
    */admin.py
    */apps.py
    */__pycache__/*
    */venv/*
    */env/*
    manage.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    if settings.DEBUG
    @abstractmethod
    pass

show_missing = True
precision = 2

[html]
directory = htmlcov
